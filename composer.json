{
    "name": "sonatra/gluon-bundle",
    "type": "symfony-bundle",
    "description": "A complete web responsive interface using Twitter Bootstrap",
    "keywords": ["web interface", "ui", "responsive", "admin", "landing", "twitter", "bootstrap", "website"],
    "homepage": "https://github.com/sonatra/sonatra-gluon-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1.0",
        "symfony/framework-bundle": "^3.4.0|^4.0.0",
        "symfony/templating": "^3.4.0|^4.0.0",
        "sonatra/block-bundle": "^1.0.0",
        "sonatra/routing-extra-bundle": "^1.0.0",
        "sonatra/form-extensions-bundle": "^1.0.0",
        "sonatra/bootstrap-bundle": "^1.0.0",
        "sonatra/ajax-bundle": "^1.0.0",
        "sonatra/gluon": "^1.0.0"

    },
    "require-dev": {
        "symfony/console": "^3.4.0|^4.0.0",
        "symfony/finder": "^3.4.0|^4.0.0",
        "foxy/foxy": "^1.0.0"
    },
    "config": {
        "foxy": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\GluonBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
